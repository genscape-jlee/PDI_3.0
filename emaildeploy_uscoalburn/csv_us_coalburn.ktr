  <step>
    <name>mwh</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>gdrprd</connection>
    <sql>select year as &quot;YEAR_ID&quot;, weekid as &quot;WEEK_ID&quot;, a.doe_id AS &quot;DOE_ID&quot;, plant &quot;PLANT_NAME&quot;, currentweekburn &quot;TOTAL_MWH&quot;, startdate &quot;START_DATE&quot;, enddate &quot;END_DATE&quot;, entity_id &quot;ENTITY_ID&quot;, ytd &quot;YTD&quot;
 from(
SELECT tbl.yr AS year, tbl.wk AS weekid, doe_id, upper((tbl.plant_name)::text) AS plant,startdate, enddate, tbl.entity_id,
case when tbl.tons &lt; 0 then 0 else tbl.mw end AS currentweekburn,
                        --CASE
                      --      WHEN tbl.mw &lt; ((0.25 * tbl.nameplate * (24)::numeric * (7)::numeric))::double precision THEN (0)::double precision
                         --   ELSE tbl.tons                        END AS currentweekburn, 
rank() OVER (PARTITION BY tbl.entity_id, tbl.plant_name ORDER BY tbl.yr DESC, tbl.wk DESC) AS ord, 
                        first_value(tbl.wk) OVER (PARTITION BY tbl.plant_name ORDER BY tbl.yr DESC, tbl.wk DESC) AS lyw, 
                        first_value(tbl.yr) OVER (PARTITION BY tbl.plant_name ORDER BY tbl.yr DESC, tbl.wk DESC) - (1)::numeric AS lyy,
                        dense_rank() over (partition by tbl.plant_name order by tbl.yr desc) as ty
                   FROM ( SELECT a.entity_id, c.plant_name, my_coalburn_year(a.gdate, &apos;thursday&apos;::character varying) AS yr, b.nameplate,c.doe_id,
                    my_coalburn_week(a.gdate, &apos;thursday&apos;::character varying) AS wk, max(a.gdate) AS enddate, min(a.gdate) AS startdate,
                   sum(COALESCE(a.gen_reported_burn, a.eia_tons)) AS tons, sum(case when a.genscape_mw &lt; 0 then 0 else a.genscape_mw end) AS mw
                           FROM t_coal_repository a
