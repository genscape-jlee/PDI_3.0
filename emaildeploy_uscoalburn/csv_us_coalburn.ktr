    <connection>OilStorageDS</connection>
    <sql>select riv.headline as &quot;HEADLINE&quot;, to_char(issue_date,&apos;yyyy-mm-dd&apos;) &quot;DT&quot;, &apos;Volume &apos;||riv.volume ||&apos;&#47; Issue &apos;|| riv.ISSUE as &quot;VOLUME&quot;,
rc.commentary as &quot;COMMENTRY_FIRST&quot;, 
rcn.commentary as &quot;COMMENTRY_LAST&quot;
from  reports.reports_issue_volume riv 
left outer join reports.reports_commentary rc on rc.reports_issue_volume_id=riv.id 
left outer join reports.reports_image ri on ri.reports_issue_volume_id = riv.id and ri.image_id=(select min(image_id) from reports.reports_image where reports_issue_volume_id = riv.id)
left outer join reports.reports_image rin on rin.reports_issue_volume_id = riv.id and not(rin.image_id = ri.image_id) 
left outer join reports.reports_commentary rcn on rcn.reports_issue_volume_id =riv.id 
and not(rcn.commentary_id = rc.commentary_id) where riv.id = ( select max(id) from reports.reports_issue_volume where Report_id=8) and rownum =1
</sql>
    <limit>0</limit>
    <lookup/>
